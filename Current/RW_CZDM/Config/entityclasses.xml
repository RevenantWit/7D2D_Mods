<!--Spider Tag Fix-->
<conditional>
	<if cond="game_version() &gt;= version(1,2,0) and game_version() &lt; version(1,2,2)">
		<csv xpath="//entity_class[starts-with(@name, 'zombieSpider')]/property[@name='Tags'][not(contains(@value, 'spider'))]/@value" delim="," op="add" >spider</csv>
	</if>
</conditional>

	<!--Special Zombies Group Tag-->
