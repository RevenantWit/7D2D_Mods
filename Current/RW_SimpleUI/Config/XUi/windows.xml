				<include filename="RWSUI_xuiConfigs/AddOns/CraftingQueue/CraftingQueuex05/windows.xml" />
			</if>
		</conditional>
	</if>
	<if cond="mod_loaded('Gears')">
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'world', 'CraftingUI.CraftingQueueSlots') == '05_Slots'">
				<include filename="RWSUI_xuiConfigs/AddOns/CraftingQueue/CraftingQueuex05/windows.xml" />
			</if>
		</conditional>
	</if>
</conditional>
<!--Crafting_Queue_Size END-->

