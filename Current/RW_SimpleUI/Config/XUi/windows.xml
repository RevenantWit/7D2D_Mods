<!--Player_BackPack_Size START-->
<conditional>
	<if cond="mod_loaded('RWSUI_BigBag60') or mod_loaded('RWSUI_BigBag120')">
		<!-- Check for old add ons to avoid conflicts -->
		<!-- This check will get removed in a future update -->
	</if>
	<if cond="mod_loaded('RWSUI_PlayerBackPackSize')">
		<conditional>
			<if cond="mod_version('RWSUI_PlayerBackPackSize') == version(6,0)">
				<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB60/windows.xml" />
				<conditional>
					<if cond="mod_loaded('RWSUI_BBInfoPanel_Blocker') == 'false'">
						<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB60/InfoPanel/windows.xml" />
					</if>
				</conditional>
			</if>
			<if cond="mod_version('RWSUI_PlayerBackPackSize') == version(12,0)">
				<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB120/windows.xml" />
				<conditional>
					<if cond="mod_loaded('RWSUI_BBInfoPanel_Blocker') == 'false'">
						<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB120/InfoPanel/windows.xml" />
					</if>
				</conditional>
			</if>
		</conditional>
	</if>
	<if cond="mod_loaded('Gears')">
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.PlayerBackPackSize') == '60_Slots'">
				<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB60/windows.xml" />
				<conditional>
					<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.BigBagInfoPanelBlocker') == 'Off'">
						<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB60/InfoPanel/windows.xml" />
					</if>
				</conditional>
			</if>
			<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.PlayerBackPackSize') == '120_Slots'">
				<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB120/windows.xml" />
				<conditional>
					<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.BigBagInfoPanelBlocker') == 'Off'">
						<include filename="RWSUI_xuiConfigs/AddOns/BigBag/BB120/InfoPanel/windows.xml" />
					</if>
				</conditional>
			</if>
		</conditional>
	</if>
</conditional>
<!--Player_BackPack_Size END-->



<!--The vanilla buffs panel using the BuffPopoutList controller MUST exist somewhere in the HUD.-->
<!--Since I don't like the vanilla buffs slow slide up effect I force hide the vanilla buffs panel so it's never rendered-->
<!--Only the last vanilla buffs panel using the BuffPopoutList controller in the left HUD window will use the default slide up effect when crouched.-->
<!--To prevent the slide up effect happening elsewhere I always leave this hidden vanilla buff panel at the bottom of Simple UI's left hud window -->
<append xpath="//window[@name='RWSUI_LeftHUD']">
	<rect name="Hidden" force_hide="true" >
		<rect name="hud" pos="90,132" side="left" controller="BuffPopoutList" pivot="BottomLeft" >
			<panel width="168" height="46" name="item" visible="false" pivot="right" disableautobackground="true" pos="70, 0" style="press" sound="[recipe_click]" on_hover="true" >
				<sprite depth="3" pos="0,0"  name="Background" sprite="ui_game_popup" height="46" width="162" pivot="center" flip="Horizontally" color="[transparent]" />
				<sprite depth="4" name="Icon" size="40,40" pos="-54,0" pivot="center" color="[transparent]"/>
				<label depth="6" name="TextContent" pos="-37,0" font_size="28" color="[white]" justify="center" width="118" height="40" pivot="left"/>
			</panel>
		</rect>
	</rect>
</append>

</configs>
