<configs>

<append xpath="/windows">

	<window name="RWSUI_ToolBelt10" anchor="CenterBottom" pos="-376,96" cursor_area="true">
		<rect name="windowToolbelt" width="753" height="78" depth="5" backgroundcolor="[black]" controller="ToolbeltWindow" standard_xp_color="[rwsui_xpbar_standard_color]" updating_xp_color="[rwsui_xpbar_updating_color]" deficit_xp_color="[rwsui_xpbar_deficit_color]" xp_fill_speed="2.5" >

			<sprite depth="8" name="backgroundMain" sprite="menu_empty3px" color="[black]" type="sliced" fillcenter="false" on_press="true" gamepad_selectable="false" />
			
			<!--Simple Toolbelt-->
			<rwsui_toolbelt pos="3,-3" cols="10" clear_inventory1_pos="738,19" clear_inventory2_pos="800,50"/>

			<!--Stealth Bar when nothing is alerted to player-->
			<rect name="StealthBar01" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor == '50,135,0,255'}">
				<rwsui_hud_doubleNoText_stealthBar pos="376,20" width="388" height="22" barContent_color="[rwsui_stealthbar_content_safeColor]" sprite="{staticon|once}" icon_color="[white]" icon_size="18,18" icon_pos="0,-10" />
			</rect>

			<!--Stealth Bar when something is alerted to the player-->
			<rect name="StealthBar02" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor != '50,135,0,255'}">
				<rwsui_hud_doubleNoText_stealthBar pos="376,20" width="388" height="22" barContent_color="[rwsui_stealthbar_content_dangerColor]" sprite="{staticon|once}" icon_color="[white]" icon_size="18,18" icon_pos="0,-10" />
			</rect>

			<!--XP Bar-->
			<rwsui_toolbelt_xpBar pos="0,-77" filldirection="horizontal" width="753" height="18" barBackground_color="[rwsui_xpbar_background_color]" sprite="ui_game_symbol_trophy" icon_color="[rwsui_xpbar_icon_color]" icon_size="14,14" icon_pos="${Round(width/2, 0)-50},-9" text="{# localization('xuiLevel')}    {cvar($LastPlayerLevel)}" text_pos="0,-1" text_width="749" font_size="16" text_height="16" text_color="[rwsui_xpbar_text_color]" textOutline_color="[rwsui_xpbar_textOutline_color]" textOutline_size="[rwsui_xpbar_textOutline_size]" text_justify="center" />

		</rect>
		<!--Reverse Fill Horizontal Stamina Bar-->
		<rwsui_hud_statBar_reverseFillH pos="-171,0" stat_type="Stamina" barBackground_fill="{statmodifiedmax}" filldirection="horizontal" width="171" height="47" barBackground_color="[rwsui_staminabar_background_color]" barContent_color="[rwsui_staminabar_content_color]" icon_pos="145,-24" icon_size="35,35" sprite="{# sprintactive ? 'server_z_always_run' : 'ui_game_symbol_agility'}" icon_color="[rwsui_staminabar_icon_color]" text_pos="-18,-13" text_width="171" font_size="26" text_height="24" text="{statcurrentwithmax}" text_color="[rwsui_staminabar_text_color]" textOutline_color="[rwsui_staminabar_textOutline_color]" textOutline_size="[rwsui_staminabar_textOutline_size]" text_justify="center" />

		<!--Reverse Fill Horizontal Health Bar-->
		<rwsui_hud_statBar_reverseFillH pos="-171,-48" stat_type="Health" barBackground_fill="{statmodifiedmax}" filldirection="horizontal" width="171" height="47" barBackground_color="[rwsui_healthbar_background_color]" barContent_color="[rwsui_healthbar_content_color]" icon_pos="145,-24" icon_size="35,35" sprite="{staticon|once}" icon_color="[rwsui_healthbar_icon_color]" text_pos="-18,-13" text_width="171" font_size="26" text_height="24" text="{statcurrentwithmax}" text_color="[rwsui_healthbar_text_color]" textOutline_color="[rwsui_healthbar_textOutline_color]" textOutline_size="[rwsui_healthbar_textOutline_size]" text_justify="center" />

		<!--Horizontal Food Bar-->
		<rwsui_hud_statBar pos="753,0" stat_type="Food" barBackground_fill="1" filldirection="horizontal" width="171" height="47" barBackground_color="[rwsui_foodbar_background_color]" barContent_color="[rwsui_foodbar_content_color]" icon_pos="26,-24" icon_size="35,35" sprite="{staticon|once}" icon_color="[rwsui_foodbar_icon_color]" text_pos="13,-13" text_width="171" font_size="26" text_height="24" text="{# statcurrent &lt; '99' ? statcurrent : '100'}%" text_color="[rwsui_foodbar_text_color]" textOutline_color="[rwsui_foodbar_textOutline_color]" textOutline_size="[rwsui_foodbar_textOutline_size]" text_justify="center" />

		<!--Horizontal Water Bar-->
		<rwsui_hud_statBar pos="753,-48" stat_type="Water" barBackground_fill="1" filldirection="horizontal" width="171" height="47" barBackground_color="[rwsui_waterbar_background_color]" barContent_color="[rwsui_waterbar_content_color]" icon_pos="26,-24" icon_size="35,35" sprite="{staticon|once}" icon_color="[rwsui_waterbar_icon_color]" text_pos="13,-13" text_width="171" font_size="26" text_height="24" text="{# statcurrent &lt; '99' ? statcurrent : '100'}%" text_color="[rwsui_waterbar_text_color]" textOutline_color="[rwsui_waterbar_textOutline_color]" textOutline_size="[rwsui_waterbar_textOutline_size]" text_justify="center" />

	</window>

