<configs>

<append xpath="/windows">

	<window name="RWSUI_LeftHUD" anchor="LeftBottom" pos="5,146" >
		<rect name="leftHUDwindow" side="right">
			<!--Radial Stealth Bar when nothing is alerted to player-->
			<rect name="StealthBar01" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor == '50,135,0,255'}">
				<rwsui_hud_statBarR pos="160,11" stat_type="Stealth" barBackground_fill="1" width="75" height="75" border_color="[rwsui_stealthbar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_stealthbar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_stealthbar_background_color]" barContent_color="[rwsui_stealthbar_content_safeColor]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_stealthbar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="20,20" sprite="{staticon|once}" icon_color="[rwsui_stealthbar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{statcurrent}" text_color="[rwsui_stealthbar_text_color]" textOutline_color="[rwsui_stealthbar_textOutline_color]" textOutline_size="[rwsui_stealthbar_textOutline_size]" text_justify="center" />
			</rect>

			<!--Radial Stealth Bar when something is alerted to the player-->
			<rect name="StealthBar02" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor != '50,135,0,255'}">
				<rwsui_hud_statBarR pos="160,11" stat_type="Stealth" barBackground_fill="1" width="75" height="75" border_color="[rwsui_stealthbar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_stealthbar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_stealthbar_background_color]" barContent_color="[rwsui_stealthbar_content_dangerColor]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_stealthbar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="20,20" sprite="{staticon|once}" icon_color="[rwsui_stealthbar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{statcurrent}" text_color="[rwsui_stealthbar_text_color]" textOutline_color="[rwsui_stealthbar_textOutline_color]" textOutline_size="[rwsui_stealthbar_textOutline_size]" text_justify="center" />
			</rect>

			<!--Radial Stamina Bar-->
			<rwsui_hud_statBarR pos="-1,11" stat_type="Stamina" barBackground_fill="{statmodifiedmax}" width="75" height="75" border_color="[rwsui_staminabar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_staminabar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_staminabar_background_color]" barContent_color="[rwsui_staminabar_content_color]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_staminabar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="20,20" sprite="{# sprintactive ? 'server_z_always_run' : 'ui_game_symbol_agility'}" icon_color="[rwsui_staminabar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{# length(statcurrent)==0 ? '' : Round(float(statcurrent), 0)}" text_color="[rwsui_staminabar_text_color]" textOutline_color="[rwsui_staminabar_textOutline_color]" textOutline_size="[rwsui_staminabar_textOutline_size]" text_justify="center" />

			<!--Radial Health Bar-->
			<rwsui_hud_statBarR pos="80,11" stat_type="Health" barBackground_fill="{statmodifiedmax}" width="75" height="75" border_color="[rwsui_healthbar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_healthbar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_healthbar_background_color]" barContent_color="[rwsui_healthbar_content_color]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_healthbar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="20,20" sprite="{staticon|once}" icon_color="[rwsui_healthbar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{statcurrent}" text_color="[rwsui_healthbar_text_color]" textOutline_color="[rwsui_healthbar_textOutline_color]" textOutline_size="[rwsui_healthbar_textOutline_size]" text_justify="center" />

			<!--Radial Food Bar-->
			<rwsui_hud_statBarR pos="-1,-68" stat_type="Food" barBackground_fill="1" width="75" height="75" border_color="[rwsui_foodbar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_foodbar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_foodbar_background_color]" barContent_color="[rwsui_foodbar_content_color]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_foodbar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="20,20" sprite="{staticon|once}" icon_color="[rwsui_foodbar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{# statcurrent &lt; '99' ? statcurrent : '100'}%" text_color="[rwsui_foodbar_text_color]" textOutline_color="[rwsui_foodbar_textOutline_color]" textOutline_size="[rwsui_foodbar_textOutline_size]" text_justify="center" />

			<!--Radial Water Bar-->
			<rwsui_hud_statBarR pos="80,-68" stat_type="Water" barBackground_fill="1" width="75" height="75" border_color="[rwsui_waterbar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_waterbar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_waterbar_background_color]" barContent_color="[rwsui_waterbar_content_color]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_waterbar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="20,20" sprite="{staticon|once}" icon_color="[rwsui_waterbar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{# statcurrent &lt; '99' ? statcurrent : '100'}%" text_color="[rwsui_waterbar_text_color]" textOutline_color="[rwsui_waterbar_textOutline_color]" textOutline_size="[rwsui_waterbar_textOutline_size]" text_justify="center" />

			<!--Radial Xp Bar-->
			<rwsui_hud_xpBarR pos="160,-68" xp_fill_speed="2" width="75" height="75" standard_xp_color="[rwsui_xpbar_standard_color]" updating_xp_color="[rwsui_xpbar_updating_color]" deficit_xp_color="[rwsui_xpbar_deficit_color]" border_color="[rwsui_xpbar_border_color]" border_thickness="4" border_globalopacity="false" borderCover_color="[rwsui_xpbar_borderCover_color]" borderCover_globalopacity="false" barBackground_color="[rwsui_xpbar_background_color]" topLayer_pos="12,-12" topLayer_height="51" topLayer_width="51" topLayer_color="[rwsui_xpbar_topLayer_color]" topLayer_globalopacity="false" icon_pos="${Round(width/2, 0)},-50" icon_size="18,18" sprite="ui_game_symbol_trophy" icon_color="[rwsui_xpbar_icon_color]" text_pos="2,-21" text_width="71" font_size="18" text_height="16" text="{cvar($LastPlayerLevel)}" text_color="[rwsui_xpbar_text_color]" textOutline_color="[rwsui_xpbar_textOutline_color]" textOutline_size="[rwsui_xpbar_textOutline_size]" text_justify="center" />
		</rect>

	</window>

</append>

</configs>