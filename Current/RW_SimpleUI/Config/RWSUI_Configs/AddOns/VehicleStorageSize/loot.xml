<configs>
<!-- vehicle storage max is 90 slots in vanilla UI windows.xml -->

<!--Bicycle_Storage_Size-->
<conditional>
	<if cond="mod_loaded('RWSUI_BicycleStorageSize')">
		<conditional>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(1,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,1</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(2,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,2</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(3,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,3</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(4,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,4</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(5,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,5</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(6,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,6</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(7,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,7</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(8,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,8</set>
			</if>
			<if cond="mod_version('RWSUI_BicycleStorageSize') == version(9,0)">
				<set xpath="/lootcontainers/lootcontainer[@name='vehicleBicycle']/@size">10,9</set>
			</if>
		</conditional>
	</if>
</conditional>

