<?xml version="1.0" encoding="utf-8"?>
<ModSettings>
  <Global>
    <Tab name="HUD" displayKey="rwsuiHUD_Tab" tooltipKey="rwsuiHUD_Tab_Desc">
      <Category name="StatBars" displayKey="rwsuiStatBar_Category" tooltipKey="rwsuiStatBar_Category_Desc">
        <Selector name="StatBarType" displayKey="rwsuiStatBar_Option" tooltipKey="rwsuiStatBar_Option_Desc" defaultValue="Horizontal">
          <Property type="arrayValues" allowedValues="Horizontal,Vertical,Round,MiniHUD" />
        </Selector>
        <Selector name="StatBarLocation" displayKey="rwsuiStatBarLocation_Option" tooltipKey="rwsuiStatBarLocation_Option_Desc" defaultValue="Bottom_Left">
          <Property type="arrayValues" allowedValues="Bottom_Left,ToolBelt" />
        </Selector>
      </Category>
      <Category name="BuffPanels" displayKey="rwsuiBuffPanel_Category" tooltipKey="rwsuiBuffPanel_Category_Desc">
        <Selector name="BuffPanel" displayKey="rwsuiBuffPanel_Option" tooltipKey="rwsuiBuffPanel_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,Simple_Buffs,Detailed_Buffs" />
        </Selector>
      </Category>
    </Tab>
    <Tab name="Labels" displayKey="rwsuiLabels_Tab" tooltipKey="rwsuiLabels_Tab_Desc">
      <Category name="StatBars" displayKey="rwsuiStatBarsLabel_Category" tooltipKey="rwsuiStatBarsLabel_Category_Desc">
        <Switch name="StatBarText" displayKey="rwsuiStatBarText_Toggle" tooltipKey="rwsuiStatBarText_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
        <Switch name="StatBarIcons" displayKey="rwsuiStatBarIcons_Toggle" tooltipKey="rwsuiStatBarIcons_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
      </Category>
      <Category name="ToolBelt" displayKey="rwsuiToolBeltLabel_Category" tooltipKey="rwsuiToolBeltLabel_Category_Desc">
        <Switch name="ToolBeltKeys" displayKey="rwsuiToolBeltKeys_Toggle" tooltipKey="rwsuiToolBeltKeys_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
        <Switch name="ToolBeltKeysBG" displayKey="rwsuiToolBeltKeysBG_Toggle" tooltipKey="rwsuiToolBeltKeysBG_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
      </Category>
    </Tab>
    <Tab name="Misc" displayKey="rwsuiMisc_Tab" tooltipKey="rwsuiMisc_Tab_Desc">
      <Category name="SimpleUIBlockers" displayKey="rwsuiSimpleUIBlockers_Category" tooltipKey="rwsuiSimpleUIBlockers_Category_Desc">
        <Switch name="HUDBlocker" displayKey="rwsuiHUDBlocker_Toggle" tooltipKey="rwsuiHUDBlocker_Toggle_Desc" defaultValue="Off" leftValue="Off" rightValue="On" />
        <Switch name="ToolBeltBlocker" displayKey="rwsuiToolBeltBlocker_Toggle" tooltipKey="rwsuiToolBeltBlocker_Toggle_Desc" defaultValue="Off" leftValue="Off" rightValue="On" />
		<Switch name="VanillaTBStatBar" displayKey="rwsuiVTBStatBarBlocker_Toggle" tooltipKey="rwsuiVTBStatBarBlocker_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
      </Category>
      <Category name="PopUps" displayKey="rwsuiPopUp_Category" tooltipKey="rwsuiPopUp_Category_Desc">
        <Switch name="LocationPopUps" displayKey="rwsuiLocationPopUp_Toggle" tooltipKey="rwsuiLocationPopUp_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
      </Category>
      <Category name="OnScreenIcons" displayKey="rwsuiOnScreenIcons_Category" tooltipKey="rwsuiOnScreenIcons_Category_Desc">
        <Switch name="AlliedPlayerIcon" displayKey="rwsuiAlliedPlayerIcon_Toggle" tooltipKey="rwsuiAlliedPlayerIcon_Toggle_Desc" defaultValue="On" leftValue="Off" rightValue="On" />
      </Category>
    </Tab>
  </Global>
  <World>
      <Category name="CraftingUI" displayKey="rwsuiCraftingUI_Category" tooltipKey="rwsuiCraftingUI_Category_Desc">
        <Switch name="ForgeInputSlots" displayKey="rwsuiForgeInput3Slot_Toggle" tooltipKey="rwsuiForgeInput3Slot_Toggle_Desc" defaultValue="Off" leftValue="Off" rightValue="On" />
        <Selector name="CraftingQueueSlots" displayKey="rwsuiCraftingQueueSize_Option" tooltipKey="rwsuiCraftingQueueSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,05_Slots" />
        </Selector>
      </Category>
      <Category name="PlayerBackPack" displayKey="rwsuiPlayerBackPack_Category" tooltipKey="rwsuiPlayerBackPack_Category_Desc">
        <Switch name="BigBagInfoPanelBlocker" displayKey="rwsuiBBInfoPanelBlocker_Toggle" tooltipKey="rwsuiBBInfoPanelBlocker_Toggle_Desc" defaultValue="Off" leftValue="Off" rightValue="On" />
        <Selector name="PlayerBackPackSize" displayKey="rwsuiPlayerBackPackSize_Option" tooltipKey="rwsuiPlayerBackPackSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,60_Slots,120_Slots" />
        </Selector>
      </Category>
      <Category name="VehicleStorage" displayKey="rwsuiVehicleStorage_Category" tooltipKey="rwsuiVehicleStorage_Category_Desc">
        <Selector name="vehicleBicycle" displayKey="rwsuiBicycleStorageSize_Option" tooltipKey="rwsuiBicycleStorageSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,10_Slots,20_Slots,30_Slots,40_Slots,50_Slots,60_Slots,70_Slots,80_Slots,90_Slots" />
        </Selector>
        <Selector name="vehicleMinibike" displayKey="rwsuiMinibikeStorageSize_Option" tooltipKey="rwsuiMinibikeStorageSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,10_Slots,20_Slots,30_Slots,40_Slots,50_Slots,60_Slots,70_Slots,80_Slots,90_Slots" />
        </Selector>
        <Selector name="vehicleMotorcycle" displayKey="rwsuiMotorcycleStorageSize_Option" tooltipKey="rwsuiMotorcycleStorageSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,10_Slots,20_Slots,30_Slots,40_Slots,50_Slots,60_Slots,70_Slots,80_Slots,90_Slots" />
        </Selector>
        <Selector name="vehicle4x4Truck" displayKey="rwsui4x4TruckStorageSize_Option" tooltipKey="rwsui4x4TruckStorageSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,10_Slots,20_Slots,30_Slots,40_Slots,50_Slots,60_Slots,70_Slots,80_Slots,90_Slots" />
        </Selector>
        <Selector name="vehicleGyrocopter" displayKey="rwsuiGyrocopterStorageSize_Option" tooltipKey="rwsuiGyrocopterStorageSize_Option_Desc" defaultValue="Vanilla">
          <Property type="arrayValues" allowedValues="Vanilla,10_Slots,20_Slots,30_Slots,40_Slots,50_Slots,60_Slots,70_Slots,80_Slots,90_Slots" />
        </Selector>
      </Category>
  </World>
</ModSettings>