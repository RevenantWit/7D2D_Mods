				<include filename="rwsui_XUi/AddOns/CraftingQueue/CraftingQueuex05/windows.xml" />
			</if>
		</conditional>
	</if>
	<if cond="mod_loaded('Gears')">
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'world', 'CraftingUI.CraftingQueueSlots') == '05_Slots'">
				<include filename="rwsui_XUi/AddOns/CraftingQueue/CraftingQueuex05/windows.xml" />
			</if>
		</conditional>
	</if>
</conditional>
<!--_Crafting_Queue_Size END-->

<!--_Forge_Input_Size START-->
<conditional>
	<if cond="mod_loaded('RWSUI_ForgeInput')">
