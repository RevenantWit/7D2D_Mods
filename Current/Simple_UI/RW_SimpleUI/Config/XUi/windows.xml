<!--_Vanilla_Food_Water_Bars START-->
<conditional>
	<!-- Backwards compatibility for the way the old add on was handeled -->
	<if cond="mod_loaded('RWSUI_ToolBelt_Blocker') and mod_version('RWSUI_ToolBelt_Blocker') == version(1,1)">
		<remove xpath="//window[@name='windowToolbelt']//rect[@stat_type='Food' or @stat_type='Water']"/>
	</if>
	<!-- New method doesn't care if toolbelt blocker is installed or not but will only run if old method is not used -->
	<if cond="mod_loaded('RWSUI_VFWBarRemover')">
		<remove xpath="//window[@name='windowToolbelt']//rect[@stat_type='Food' or @stat_type='Water']"/>
	</if>
	<if cond="mod_loaded('Gears')">
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'global', 'Misc.SimpleUIBlockers.VanillaTBStatBar') == 'On'">
				<remove xpath="//window[@name='windowToolbelt']//rect[@stat_type='Food' or @stat_type='Water']"/>
			</if>
		</conditional>
	</if>
</conditional>
<!--_Vanilla_Food_Water_Bars END-->

<!--_ToolbeltKey_Labels START-->
<conditional>
	<!-- If ToolbeltKeys Remover version number is 1.1 remove only the toolbelt keys background -->
	<if cond="mod_loaded('RWSUI_ToolBeltKeys_Remover') and mod_version('RWSUI_ToolBeltKeys_Remover') == version(1,1)">
		<remove xpath="//window[@name='RWSUI_ToolBelt10']//rwsui_toolbelt_keys_bg"/>
	</if>
	<!-- If ToolbeltKeys Remover is installed remove the toolbelt keys and background from being displayed above toolbelt -->
	<if cond="mod_loaded('RWSUI_ToolBeltKeys_Remover')">
		<remove xpath="//window[@name='RWSUI_ToolBelt10']//rwsui_toolbelt_keys"/>
		<remove xpath="//window[@name='RWSUI_ToolBelt10']//rwsui_toolbelt_keys_bg"/>
	</if>
	<if cond="mod_loaded('Gears')">
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'global', 'Labels.ToolBelt.ToolBeltKeys') == 'Off'">
				<remove xpath="//window[@name='RWSUI_ToolBelt10']//rwsui_toolbelt_keys"/>
			</if>
		</conditional>
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'global', 'Labels.ToolBelt.ToolBeltKeysBG') == 'Off'">
				<remove xpath="//window[@name='RWSUI_ToolBelt10']//rwsui_toolbelt_keys_bg"/>
			</if>
		</conditional>
	</if>
</conditional>
<!--_ToolbeltKey_Labels END-->

<!--_Crafting_Queue_Size START-->
