<configs>

<conditional>
	<if cond="mod_version('RWSUI_HUD_Settings') &gt;= version(5,0) and mod_version('RWSUI_HUD_Settings') &lt; version(6,0)">
		<set xpath="//window/rect//rwsui_hud_doubleNoText_stealthBar/@icon_hidden">true</set>
		<set xpath="//window//rwsui_toolbelt_xpBar/@icon_hidden">true</set>
		<set xpath="//window/rwsui_hud_statBar/@icon_hidden">true</set>
	</if>
	<if cond="mod_version('RWSUI_HUD_Settings') &gt;= version(6,0) and mod_version('RWSUI_HUD_Settings') &lt; version(7,0)">
		<set xpath="//window//rwsui_hud_statBarR/@icon_hidden">true</set>
		<set xpath="//window/rwsui_hud_xpBarR/@icon_hidden">true</set>
		<set xpath="//window[@name='RWSUI_ToolBelt15']/rect//rwsui_hud_doubleNoText_stealthBar/@icon_hidden">true</set>
		<set xpath="//window[@name='RWSUI_ToolBelt15']//rwsui_toolbelt_xpBar/@icon_hidden">true</set>
	</if>
	<if cond="mod_version('RWSUI_HUD_Settings') &gt;= version(7,0) and mod_version('RWSUI_HUD_Settings') &lt; version(7,3)">
		<set xpath="//window/rect//rwsui_hud_doubleNoText_stealthBar/@icon_hidden">true</set>
		<set xpath="//window//rwsui_toolbelt_xpBar/@icon_hidden">true</set>
		<set xpath="//window/rwsui_hud_statBar/@icon_hidden">true</set>
	</if>
</conditional>

</configs>
