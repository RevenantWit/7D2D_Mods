<append xpath="/controls">
<!--
	<rwsui_statbar_horizontal_stamina pos="0,0" style="rwsui_statbar_stamina" staminabar_pos="0" staminabar_style="rwsui_statbar_stamina" >

		<rwsui_hud_statBar pos="${staminabar_pos != 0 ? (staminabar_pos) : (pos)}" style="rwsui_statbar, rwsui_statbar_stamina, ${style}, ${staminabar_style}" />

	</rwsui_statbar_horizontal_stamina>
-->

<!--_Stat_Bars START-->
	<rwsui_hud_statBar>
		<rect width="${width}" height="${height}" pos="${pos}" controller="${controller}" stat_type="${stat_type}" visible="{statvisible}">
			<sprite depth="0" name="border" color="0,0,0,140" type="sliced" />
			<sprite depth="1" name="border" color="0,0,0,255" type="sliced" fillcenter="false" />
			<sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

			<filledsprite depth="3" name="background" color="${barBackground_color}" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="${barBackground_fill}" />

			<filledsprite depth="4" name="BarContent" color="${barContent_color}" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="0" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" controller="HUDStatBar" stat_type="${stat_type}" />

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" force_hide="${text_hidden}" />

			<sprite depth="7" name="border" color="0,0,0,255" type="sliced" fillcenter="false"/>

			<filledsprite depth="8" name="background" color="0,0,0,255" fillcenter="false" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="${barBackground_fill}" />
			<filledsprite depth="3" name="background" color="0,0,0,255" fillcenter="false" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="${barBackground_fill}" />
		</rect>
	</rwsui_hud_statBar>

	<rwsui_hud_statBarR>
		<rect width="${width}" height="${height}" pos="${pos}" controller="${controller}" stat_type="${stat_type}" visible="{statvisible}">
			<sprite depth="0" name="border" pos="-${Round(border_thickness/2, 0)},${Round(border_thickness/2, 0)}" height="${Round(border_thickness, 0)+height}" width="${Round(border_thickness, 0)+width}" color="${border_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" />

			<sprite depth="1" name="background" pos="${centerBG_pos}" width="${centerBG_width}" height="${centerBG_height}" color="${centerBG_color}" sprite="ui_game_filled_circle" />

			<sprite depth="2" name="border" pos="-${Round(border_thickness/2, 0)},${Round(border_thickness/2, 0)}" height="${Round(border_thickness, 0)+height}" width="${Round(border_thickness, 0)+width}" color="${border_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" />

			<sprite depth="3" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="background" color="${barBackground_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="${barBackground_fill}" />

			<sprite depth="4" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="BarContent" color="${barContent_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="0" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" controller="HUDStatBar" stat_type="${stat_type}" />

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" pivot="center" force_hide="${text_hidden}" />
		</rect>
	</rwsui_hud_statBarR>

	<rwsui_hud_statBar_stealth>
		<rect width="${width}" height="${height}" pos="${pos}" controller="HUDStatBar" stat_type="Stealth" visible="{statvisible}">
			<sprite depth="0" name="border" color="0,0,0,140" type="sliced" />
			<sprite depth="1" name="border" color="0,0,0,255" type="sliced" fillcenter="false" />
			<sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

			<filledsprite depth="3" name="background" color="${barBackground_color}" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="${barBackground_fill}" />

			<filledsprite depth="4" name="BarContent" color="[rwsui_stealthbar_content_safeColor]" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="0" visible="{# stealthcolor == '50,135,0,255'}" />

			<filledsprite depth="4" name="BarContent" color="[rwsui_stealthbar_content_dangerColor]" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="0" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor != '50,135,0,255'}" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" controller="HUDStatBar" stat_type="Stealth" />

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" force_hide="${text_hidden}" />

			<sprite depth="8" name="border" color="0,0,0,255" type="sliced" fillcenter="false"/>

			<filledsprite depth="8" name="background" color="0,0,0,255" fillcenter="false" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="${barBackground_fill}" />
			<filledsprite depth="3" name="background" color="0,0,0,255" fillcenter="false" type="filled" filldirection="${filldirection}" fillinvert="${fillinvert}" fill="${barBackground_fill}" />
		</rect>
	</rwsui_hud_statBar_stealth>

	<rwsui_hud_statBarR_stealth>
		<rect width="${width}" height="${height}" pos="${pos}" controller="${controller}" stat_type="Stealth" visible="{statvisible}">
			<sprite depth="0" name="border" pos="-${Round(border_thickness/2, 0)},${Round(border_thickness/2, 0)}" height="${Round(border_thickness, 0)+height}" width="${Round(border_thickness, 0)+width}" color="${border_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" />

			<sprite depth="1" name="background" pos="${centerBG_pos}" width="${centerBG_width}" height="${centerBG_height}" color="${centerBG_color}" sprite="ui_game_filled_circle" />

			<sprite depth="2" name="border" pos="-${Round(border_thickness/2, 0)},${Round(border_thickness/2, 0)}" height="${Round(border_thickness, 0)+height}" width="${Round(border_thickness, 0)+width}" color="${border_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" />

			<sprite depth="3" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="background" color="${barBackground_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="${barBackground_fill}" />

			<sprite depth="4" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="BarContent" color="[rwsui_stealthbar_content_safeColor]" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="0" visible="{# stealthcolor == '50,135,0,255'}" />

			<sprite depth="4" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="BarContent" color="[rwsui_stealthbar_content_dangerColor]" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="0" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor != '50,135,0,255'}" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" controller="HUDStatBar" stat_type="Stealth" />

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" pivot="center" force_hide="${text_hidden}" />
		</rect>
	</rwsui_hud_statBarR_stealth>

	<rwsui_hud_toolbelt_doubleStealth>
		<rect width="${width}" height="${height}" pos="${pos}" controller="HUDStatBar" stat_type="Stealth" visible="{statvisible}">

			<filledsprite depth="4" name="BarContent" sprite="ui_game_header_insert" pos="7,0" color="[rwsui_stealthbar_content_safeColor]" type="filled" filldirection="${filldirection}" fillinvert="true" fill="0" visible="{# stealthcolor == '50,135,0,255'}" />
			<filledsprite depth="4" name="BarContent" sprite="ui_game_header_insert" pos="${width-7},0" color="[rwsui_stealthbar_content_safeColor]" type="filled" filldirection="${filldirection}" fillinvert="false" fill="0" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor == '50,135,0,255'}" />

			<filledsprite depth="4" name="BarContent" sprite="ui_game_header_insert" pos="7,0" color="[rwsui_stealthbar_content_dangerColor]" type="filled" filldirection="${filldirection}" fillinvert="true" fill="0" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor != '50,135,0,255'}" />
			<filledsprite depth="4" name="BarContent" sprite="ui_game_header_insert" pos="${width-7},0" color="[rwsui_stealthbar_content_dangerColor]" type="filled" filldirection="${filldirection}" fillinvert="false" fill="0" controller="HUDStatBar" stat_type="Stealth" visible="{# stealthcolor != '50,135,0,255'}" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" controller="HUDStatBar" stat_type="Stealth" />

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" force_hide="${text_hidden}" />
		</rect>
	</rwsui_hud_toolbelt_doubleStealth>

	<rwsui_hud_xpBar>
		<rect width="${width}" height="${height}" pos="${pos}" controller="ToolbeltWindow" standard_xp_color="${standard_xp_color}" updating_xp_color="${updating_xp_color}" deficit_xp_color="${deficit_xp_color}" xp_fill_speed="${xp_fill_speed}" >
			<sprite depth="1" name="border" color="0,0,0,128" type="sliced" />
			<sprite depth="2" name="background" color="0,0,0,140" type="sliced" />

			<filledsprite depth="3" name="background" color="${barBackground_color}" type="filled" filldirection="${filldirection}" fill="1" />

			<filledsprite depth="4" name="BarContent" color="{xpcolor}" type="filled" filldirection="${filldirection}" fill="{xp}" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}"/>

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" force_hide="${text_hidden}" />

			<sprite depth="8" name="border" color="0,0,0,255" type="sliced" fillcenter="false"/>

			<filledsprite depth="8" name="background" color="0,0,0,255" width="${width}" height="${height}" fillcenter="false" type="filled" filldirection="${filldirection}" fill="1" />
			<filledsprite depth="3" name="background" color="0,0,0,255" width="${width-1}" height="${height-1}" fillcenter="false" type="filled" filldirection="${filldirection}" fill="1" />
		</rect>
	</rwsui_hud_xpBar>

	<rwsui_hud_xpBarR>
		<rect width="${width}" height="${height}" pos="${pos}" controller="ToolbeltWindow" standard_xp_color="${standard_xp_color}" updating_xp_color="${updating_xp_color}" deficit_xp_color="${deficit_xp_color}" xp_fill_speed="${xp_fill_speed}" >
			<sprite depth="0" name="border" pos="-${Round(border_thickness/2, 0)},${Round(border_thickness/2, 0)}" height="${Round(border_thickness, 0)+height}" width="${Round(border_thickness, 0)+width}" color="${border_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" />

			<sprite depth="1" name="background" pos="${centerBG_pos}" width="${centerBG_width}" height="${centerBG_height}" color="${centerBG_color}" sprite="ui_game_filled_circle" />

			<sprite depth="2" name="border" pos="-${Round(border_thickness/2, 0)},${Round(border_thickness/2, 0)}" height="${Round(border_thickness, 0)+height}" width="${Round(border_thickness, 0)+width}" color="${border_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" />

			<sprite depth="3" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="background" color="${barBackground_color}" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="${barBackground_fill}" />

			<sprite depth="4" pos="${Round(border_thickness/2, 0)},-${Round(border_thickness/2, 0)}" height="${height-Round(border_thickness, 0)}" width="${width-Round(border_thickness, 0)}" name="BarContent" color="{xpcolor}" sprite="ui_game_symbol_twitch_overhead_hp_ring" type="filled" filldirection="radial360" fillinvert="${fillinvert}" fill="{xp}" />

			<sprite depth="5" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" />

			<label depth="6" name="TextContent" pos="${text_pos}" color="${text_color}" justify="${text_justify}" text="${text}" width="${text_width}" font_size="${font_size}" height="${text_height}" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" pivot="center" force_hide="${text_hidden}" />
		</rect>
	</rwsui_hud_xpBarR>

	<rwsui_hud_statBar_noFill>
		<rect width="${width}" height="${height}" pos="${pos}" controller="${controller}" stat_type="${stat_type}" visible="{statvisible}">
			<sprite depth="0" name="border" sprite="${border_sprite}" color="${border_color}" type="sliced" fillcenter="${fillcenter}" />
			<sprite depth="${background_depth}" name="background" pos="${background_pos}" width="${background_width}" height="${background_height}" sprite="${background_sprite}" color="${background_color}" type="sliced" />
			<sprite depth="${border_depth}" name="border" sprite="${border_sprite}" color="${border_color}" type="sliced" fillcenter="${fillcenter}" />
			<sprite depth="3" name="Icon" atlas="{staticonatlas|once}" sprite="${icon}" color="${icon_color}" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" force_hide="${icon_hidden}" controller="HUDStatBar" stat_type="${stat_type}" />
			<label depth="4" name="Text" pos="${text_pos}" width="${text_width}" font_size="${font_size}" height="${text_height}" color="${text_color}" text="${text}" justify="${text_justify}" pivot="center" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" force_hide="${text_hidden}" />
		</rect>
	</rwsui_hud_statBar_noFill>
<!--_Stat_Bars END-->

	<rwsui_hud_activeItemGrid>
		<grid name="hud" pos="${pos}" rows="2" cols="1" width="${width}" cell_width="${width}" cell_height="${height}" repeat_content="false" side="right">
			<rwsui_hud_statBar_noFill style="${activeItemGrid_style}" />
		</grid>
	</rwsui_hud_activeItemGrid>

	<rwsui_hud_activeItemGrid_vanilla>
		<grid name="hud" pos="${pos}" rows="2" cols="1" width="168" cell_width="168" cell_height="46" repeat_content="false" side="right">
			<rect width="188" height="46" controller="HUDStatBar" stat_type="ActiveItem" visible="{statvisible}">
				<sprite depth="3" pos="3,0"  name="BarContent" sprite="{statimage|once}" height="46" width="182" />
				<sprite depth="4" name="Icon" atlas="{staticonatlas|once}" sprite="{staticon}" size="39,35" pos="135,-6" color="{staticoncolor}" foregroundlayer="true" />
				<label depth="6" name="TextContent" pos="120,-28" font_size="28" color="[white]" justify="right" pivot="right" text="{statcurrentwithmax}" width="250" height="26" />
			</rect>
		</grid>
	</rwsui_hud_activeItemGrid_vanilla>

<!--_Collected_Item_List START-->
	<rwsui_hud_CIL_panel>
		<panel width="${width}" height="${height}" name="item" visible="false" pivot="right" disableautobackground="true" pos="70, 0" >
			<sprite depth="0" name="border" sprite="${border_sprite}" color="${border_color}" type="sliced" fillcenter="${fillcenter}" />
			<sprite depth="${background_depth}" name="background" pos="${background_pos}" width="${background_width}" height="${background_height}" sprite="${background_sprite}" color="${background_color}" type="sliced" />
			<sprite depth="${border_depth}" name="border" sprite="${border_sprite}" color="${border_color}" type="sliced" fillcenter="${fillcenter}" />
			<sprite depth="3" name="Icon" atlas="ItemIconAtlas" size="${icon_size}" pos="${icon_pos}" pivot="center" foregroundlayer="true" />
			<label depth="4" name="TextContent" pos="${text_pos}" width="${text_width}" font_size="${font_size}" height="${text_height}" color="${text_color}" justify="${text_justify}" pivot="center" effect="Outline8" effect_color="${textOutline_color}" effect_distance="${textOutline_size}" />
			<sprite depth="4" pos="${X_pos}" pivot="center" width="${X_width}" height="${X_height}" name="Negative" sprite="ui_game_symbol_x" color="${X_color}" />
		</panel>
	</rwsui_hud_CIL_panel>

	<rwsui_hud_collectedItemList>
		<rect name="CollectedItemList" controller="CollectedItemList" pos="${pos}" side="right" pivot="BottomLeft">
			<rwsui_hud_CIL_panel style="${CIL_panel_style}" />
		</rect>
	</rwsui_hud_collectedItemList>

	<rwsui_hud_collectedItemList_vanilla>
		<rect name="hud" pos="${pos}" side="right" controller="CollectedItemList" pivot="BottomLeft">
			<panel width="168" height="46" name="item" visible="false" pivot="right" disableautobackground="true" pos="70, 0" >
				<sprite depth="1" pos="0,0"  name="Background" sprite="ui_game_popup" height="44" width="162" pivot="center" />
				<sprite depth="2" name="Icon" atlas="ItemIconAtlas"  size="38,38" pos="55,0" pivot="center" foregroundlayer="true"/>
				<label depth="3" pos="17,-2" name="TextContent" font_size="28" color="[white]" justify="right" height="26" pivot="right"/>
				<sprite depth="3" pos="-8,10" width="26" height="26" name="Negative" sprite="ui_game_symbol_x" color="[red]" />
			</panel>
		</rect>
	</rwsui_hud_collectedItemList_vanilla>
<!--_Collected_Item_List END-->

<!--_ToolBelt START-->
<conditional>
	<if cond="game_version() &gt;= version(1,2,2)">
		<rwsui_toolbelt_item_stack controller="ItemStack" on_scroll="false" allow_dropping="true" prefix_id="false" show_favorites="false">
			<rect controller="${controller}" style="rwsui_toolbelt_itemStack, hover" collider_scale="1.0" on_scroll="${on_scroll}" allow_dropping="${allow_dropping}" prefix_id="${prefix_id}" show_favorites="${show_favorites}" tooltip="{tooltip}" enabled="{# !defined('userlockmode') or !userlockmode}" visible="{# !defined('container_slots') or (int(${repeat_i}) &lt; int(container_slots)) }" >
				<sprite depth="2" name="backgroundMain" sprite="menu_empty3px" pos="-3,3" color="[rwsui_toolbelt_border_color]" type="sliced" fillcenter="false"
					width="78" height="78" />
				<sprite depth="3" name="background" sprite="menu_empty3px" color="{selectionbordercolor}" type="sliced" globalopacity="true" fillcenter="false"
					width="73" height="73" />
				<sprite depth="2" name="highlightOverlay" color="[rwsui_toolbelt_background_color]" pos="-1,1" type="sliced" globalopacity="true"
					width="74" height="74" />

				<texture depth="3" name="backgroundTexture" material="Materials/Transparent Colored" globalopacity="true"
					size="73,73" />
				<sprite depth="8" name="favoriteIcon" width="24" height="24" sprite="server_favorite" pos="2,-2" foregroundlayer="true" visible="{isfavorite}"/>
				<sprite depth="6" name="itemIcon" width="64" height="64" atlas="ItemIconAtlas" sprite="{itemicon}" pivot="center" foregroundlayer="true" color="{iconcolor}"
					pos="35,-35" />
				<sprite depth="8" name="lockTypeIcon" width="24" height="24" sprite="{locktypeicon}" pos="2,-2" foregroundlayer="true"/>
				<sprite depth="8" name="itemtypeicon" width="24" height="24" sprite="ui_game_symbol_{itemtypeicon}" pos="2,-2" foregroundlayer="true" visible="{hasitemtypeicon}" color="{itemtypeicontint}" />

				<sprite depth="3" name="durabilityBackground" sprite="menu_empty3px" color="[darkGrey]" type="sliced" foregroundlayer="true" visible="{hasdurability}"
					width="70" height="10" pos="1, -60" />
				<filledsprite depth="4" name="durability" sprite="menu_empty2px" color="{durabilitycolor}" fill="{durabilityfill}" type="filled" foregroundlayer="true" visible="{hasdurability}"
					width="70" height="10" pos="1, -60" />

				<sprite depth="5" name="disabledOverlay" color="[darkGrey_half_alpha]" pos="1,-1" type="sliced" visible="false"
					width="72" height="72" />

				<label depth="7" name="stackValue" height="28" text="{itemcount}" font_size="24" effect="outline" justify="{#hasdurability ? 'Center' : 'Right'}"
					pos="-2,-48" width="70" />
				<label depth="7" name="timer" height="24" font_size="24" justify="center" pivot="center" color="[beige]" effect="outline"
					pos="35,-38" width="70" effect_distance="1,1" effect_color="0,0,0,255" />
				<sprite depth="9" name="cancel" sprite="ui_game_symbol_x" color="255,0,0,255" type="sliced" visible="{# ishovered and islocked}" foregroundlayer="true"
					width="70" height="70" />
				<sprite depth="13" name="assembleIcon" sprite="{stacklockicon}" color="{stacklockcolor}" visible="{isassemblelocked}" pos="14,-14" foregroundlayer="true"
					size="40,40" />

				<sprite depth="14" name="quickswapIcon" size="30,30" sprite="server_refresh" pos="40,-2" foregroundlayer="true" visible="{isQuickSwap}" />

				<button name="rectSlotLock" depth="15" style="press, hover" width="73" height="73" visible="{# defined('userlockmode') and userlockmode}" sprite="" hoverscale="1" use_selection_box="true">
					<sprite depth="16" name="slotLockIcon" width="24" height="24" sprite="{# !defined('userlockmode') ? '' : (userlockedslot ? 'ui_game_symbol_lock' : 'ui_game_symbol_unlock') }" pos="50, -14" style="icon32px" pivot="center" color="{# !defined('userlockmode') ? '255,255,255,1' : (userlockedslot ? '222,206,163' : '255,255,255') }" />
				</button>
