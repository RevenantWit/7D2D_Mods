<configs>
<!--CONTENTS START-->
<!---->
<!--_Core_Templates-->
<!--_Player_BackPack_Size-->
<!---->
<!--CONTENTS END-->

<!--_Core_Templates Start-->
<include filename="rwsui_XUi/0_Core/controls.xml"/>

<include filename="OLD_RWSUI_xuiConfigs/_Core/controls.xml"/>
<!--_Core_Templates END-->

<!--_Player_BackPack_Size START-->
<conditional>
	<if cond="mod_loaded('RWSUI_BigBag60') or mod_loaded('RWSUI_BigBag120')">
		<!-- Check for old add ons to avoid conflicts -->
		<!-- This check will get removed in a future update -->
	</if>
	<if cond="mod_loaded('RWSUI_PlayerBackPackSize')">
		<conditional>
			<if cond="mod_loaded('RWSUI_BBInfoPanel_Blocker') == 'false'">
				<conditional>
					<if cond="mod_version('RWSUI_PlayerBackPackSize') == version(6,0)">
						<include filename="rwsui_XUi/AddOns/BigBag/BB60/InfoPanel/controls.xml" />
					</if>
					<if cond="mod_version('RWSUI_PlayerBackPackSize') == version(12,0)">
						<include filename="rwsui_XUi/AddOns/BigBag/BB120/InfoPanel/controls.xml" />
					</if>
				</conditional>
			</if>
		</conditional>
	</if>
	<if cond="mod_loaded('Gears')">
		<conditional>
			<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.BigBagInfoPanelBlocker') == 'Off'">
				<conditional>
					<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.PlayerBackPackSize') == '60_Slots'">
						<include filename="rwsui_XUi/AddOns/BigBag/BB60/InfoPanel/controls.xml" />
					</if>
					<if cond="modsetting('RW_SimpleUI', 'world', 'PlayerBackPack.PlayerBackPackSize') == '120_Slots'">
						<include filename="rwsui_XUi/AddOns/BigBag/BB120/InfoPanel/controls.xml" />
					</if>
				</conditional>
			</if>
		</conditional>
	</if>
</conditional>
<!--_Player_BackPack_Size END-->

</configs>