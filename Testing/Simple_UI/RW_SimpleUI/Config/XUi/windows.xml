	<if cond="mod_loaded('RWSUI_ToolBelt_Blocker') and mod_version('RWSUI_ToolBelt_Blocker') == version(1,1)">
		<remove xpath="//window[@name='windowToolbelt']//rect[@stat_type='Food' or @stat_type='Water']"/>
	</if>
	<!-- New method doesn't care if toolbelt blocker is installed or not but will only run if old method is not used -->
	<if cond="mod_loaded('RWSUI_VFWBarRemover')">
		<remove xpath="//window[@name='windowToolbelt']//rect[@stat_type='Food' or @stat_type='Water']"/>
