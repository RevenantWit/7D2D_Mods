	<style_entry name="rwsui_collectedItemListR_border_color" value="[white]"/>
	<style_entry name="rwsui_collectedItemList_topLayer_color" value="[black]"/>
<!--_Collected_Item_List END-->

<!--_Active_Item START-->
	<style_entry name="rwsui_activeItem_border_color" value="[black]"/>
	<style_entry name="rwsui_activeItem_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_activeItem_text_color" value="[white]"/>
	<style_entry name="rwsui_activeItem_textOutline_color" value="[black]"/>
