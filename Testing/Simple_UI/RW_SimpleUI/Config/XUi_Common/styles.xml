<!--_ToolBelt_Keys END-->

<!--_Vehicle_Fuel_Bar START-->
	<style_entry name="rwsui_vehicleFuel_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_vehicleFuel_content_color" value="0,54,255,200"/>
	<style_entry name="rwsui_vehicleFuel_icon_color" value="[white]"/>
	<style_entry name="rwsui_vehicleFuel_text_color" value="[white]"/>
	<style_entry name="rwsui_vehicleFuel_textOutline_color" value="[black]"/>
