<!--_Vehicle_Health_Bar START-->
	<style_entry name="rwsui_vehicleHealth_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_vehicleHealth_content_color" value="255,0,0,200"/>
	<style_entry name="rwsui_vehicleHealth_icon_color" value="[white]"/>
	<style_entry name="rwsui_vehicleHealth_text_color" value="[white]"/>
	<style_entry name="rwsui_vehicleHealth_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_vehicleHealth_textOutline_size" value="1,1"/>
	<!--only applies to radial health bars-->
