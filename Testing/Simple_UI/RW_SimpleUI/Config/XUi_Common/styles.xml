	<style_entry name="rwsui_vehicleHealth_centerBG_color" value="[black]"/>
<!--_Vehicle_Health_Bar END-->

<!--_Collected_Item_List START-->
	<style_entry name="rwsui_collectedItemList_border_color" value="[black]"/>
	<style_entry name="rwsui_collectedItemList_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_collectedItemList_text_color" value="[white]"/>
	<style_entry name="rwsui_collectedItemList_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_collectedItemList_textOutline_size" value="1,1"/>
	<style_entry name="rwsui_collectedItemList_X_color" value="[red]"/>
