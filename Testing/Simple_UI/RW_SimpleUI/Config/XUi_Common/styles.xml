<append xpath="/styles/global">

<!--_Stealth_Bar START-->
	<style_entry name="rwsui_stealthbar_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_stealthbar_content_safeColor" value="50,135,0,255"/>
	<style_entry name="rwsui_stealthbar_content_dangerColor" value="180,180,0,255"/>
	<style_entry name="rwsui_stealthbar_icon_color" value="[white]"/>
	<style_entry name="rwsui_stealthbar_text_color" value="[white]"/>
	<style_entry name="rwsui_stealthbar_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_stealthbar_textOutline_size" value="1,1"/>
	<!--only applies to radial stealth bars-->
	<style_entry name="rwsui_stealthbar_border_color" value="[black]"/>
	<style_entry name="rwsui_stealthbar_centerBG_color" value="[black]"/>
<!--_Stealth_Bar END-->

<!--_Stamina_Bar START-->
	<style_entry name="rwsui_staminabar_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_staminabar_content_color" value="0,54,255,200"/>
	<style_entry name="rwsui_staminabar_icon_color" value="[white]"/>
	<style_entry name="rwsui_staminabar_text_color" value="[white]"/>
	<style_entry name="rwsui_staminabar_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_staminabar_textOutline_size" value="1,1"/>
	<!--only applies to radial stamina bars-->
	<style_entry name="rwsui_staminabar_border_color" value="[black]"/>
	<style_entry name="rwsui_staminabar_centerBG_color" value="[black]"/>
<!--_Stamina_Bar END-->

<!--_Health_Bar START-->
	<style_entry name="rwsui_healthbar_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_healthbar_content_color" value="255,0,0,200"/>
	<style_entry name="rwsui_healthbar_icon_color" value="[white]"/>
	<style_entry name="rwsui_healthbar_text_color" value="[white]"/>
	<style_entry name="rwsui_healthbar_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_healthbar_textOutline_size" value="1,1"/>
	<!--only applies to radial health bars-->
	<style_entry name="rwsui_healthbar_border_color" value="[black]"/>
	<style_entry name="rwsui_healthbar_centerBG_color" value="[black]"/>
<!--_Health_Bar END-->

<!--_Food_Bar START-->
	<style_entry name="rwsui_foodbar_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_foodbar_content_color" value="0,255,54,180"/>
	<style_entry name="rwsui_foodbar_icon_color" value="[white]"/>
	<style_entry name="rwsui_foodbar_text_color" value="[white]"/>
	<style_entry name="rwsui_foodbar_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_foodbar_textOutline_size" value="1,1"/>
	<!--only applies to radial food bars-->
	<style_entry name="rwsui_foodbar_border_color" value="[black]"/>
	<style_entry name="rwsui_foodbar_centerBG_color" value="[black]"/>
<!--_Food_Bar END-->

<!--_Water_Bar START-->
	<style_entry name="rwsui_waterbar_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_waterbar_content_color" value="64,164,223,200"/>
	<style_entry name="rwsui_waterbar_icon_color" value="[white]"/>
	<style_entry name="rwsui_waterbar_text_color" value="[white]"/>
	<style_entry name="rwsui_waterbar_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_waterbar_textOutline_size" value="1,1"/>
	<!--only applies to radial water bars-->
	<style_entry name="rwsui_waterbar_border_color" value="[black]"/>
	<style_entry name="rwsui_waterbar_centerBG_color" value="[black]"/>
<!--_Water_Bar END-->

<!--_Buff_Panel START-->
	<style_entry name="rwsui_buffpanel_border_color" value="[black]"/>
	<style_entry name="rwsui_buffpanel_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_buffpanel_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_buffpanel_textOutline_size" value="1,1"/>
	<style_entry name="rwsui_buffpanel_panelOffset" value="2"/>
	<!--WARNING: changing these will cause the buff icons and text to not change color depending on the severity of the buff -->
	<style_entry name="rwsui_buffpanel_icon_color" value="{buffcolor}"/>
	<style_entry name="rwsui_buffpanel_text_color" value="{buffcolor}"/>
	<!--only applies to round buff panel -->
	<style_entry name="rwsui_buffpanelR_border_color" value="[black]"/>
	<style_entry name="rwsui_buffpanel_topLayer_color" value="[black]"/>
<!--_Buff_Panel END-->

<!--_ToolBelt START-->
	<style_entry name="rwsui_toolbelt_border_color" value="[black]"/>
	<style_entry name="rwsui_toolbelt_innerborder_color" value="[transparent]"/>
	<style_entry name="rwsui_toolbelt_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_toolbelt_select_color" value="[beige]"/>
	<style_entry name="rwsui_toolbelt_holding_color" value="[green]"/>
<!--_ToolBelt END-->

<!--_XP_Bar START-->
	<style_entry name="rwsui_xpbar_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_xpbar_standard_color" value="128,4,128,255"/>
	<style_entry name="rwsui_xpbar_updating_color" value="128,4,128,255"/>
	<style_entry name="rwsui_xpbar_deficit_color" value="222,20,20,255"/>	
	<style_entry name="rwsui_xpbar_icon_color" value="[white]"/>
	<style_entry name="rwsui_xpbar_text_color" value="[white]"/>
	<style_entry name="rwsui_xpbar_textOutline_color" value="[black]"/>
	<style_entry name="rwsui_xpbar_textOutline_size" value="1,1"/>
	<!--only applies to radial xp bars-->
	<style_entry name="rwsui_xpbar_border_color" value="[black]"/>
	<style_entry name="rwsui_xpbar_centerBG_color" value="[black]"/>
<!--_XP_Bar END-->

<!--_ToolBelt_Keys START-->
	<style_entry name="rwsui_tbkeys_background_color" value="[darkestGrey]"/>
	<style_entry name="rwsui_tbkeys_text_color" value="[white]"/>
	<style_entry name="rwsui_tbkeys_textOutline_color" value="[black]"/>
